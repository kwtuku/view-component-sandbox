<p style="color: green"><%= notice %></p>

<h1>Repositories</h1>

<div id="repositories">
  <% @repositories.each do |repository| %>
    <%= render repository %>
    <p>
      <%= link_to "Show this repository", primer_repository_path(repository) %>
    </p>
  <% end %>
</div>

<%= link_to "New repository", new_primer_repository_path %>
